    <script>
        if (!document.getElementById('smk-tennis-header').dataset.scriptLoaded) {
            const menuToggleBtn = document.getElementById('menu-toggle');
            const menuIconOpen = document.getElementById('menu-icon-open');
            const menuIconClose = document.getElementById('menu-icon-close');
            const navbarMenu = document.getElementById('navbar-menu');
            const coachingToggleBtn = document.getElementById('coaching-toggle-mobile');
            const coachingSubMenu = document.getElementById('coaching-submenu-mobile');

            if (menuToggleBtn) {
                menuToggleBtn.addEventListener('click', () => {
                    const isExpanded = menuToggleBtn.getAttribute('aria-expanded') === 'true';
                    menuToggleBtn.setAttribute('aria-expanded', !isExpanded);
                    navbarMenu.classList.toggle('hidden');
                    menuIconOpen.classList.toggle('hidden');
                    menuIconClose.classList.toggle('hidden');

                    if (navbarMenu.classList.contains('hidden')) {
                        coachingSubMenu.classList.add('hidden');
                        const chevron = coachingToggleBtn.querySelector('svg');
                        if (chevron) {
                            chevron.classList.remove('rotate-180');
                        }
                    }
                });
            }

            if (coachingToggleBtn) {
                coachingToggleBtn.addEventListener('click', (e) => {
                    e.preventDefault();
                    coachingSubMenu.classList.toggle('hidden');
                    const chevron = coachingToggleBtn.querySelector('svg');
                    if (chevron) {
                        chevron.classList.toggle('rotate-180');
                    }
                });
            }

            document.getElementById('smk-tennis-header').dataset.scriptLoaded = 'true';
        }
    </script>
